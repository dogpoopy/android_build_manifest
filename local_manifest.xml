<?xml version="1.0" encoding="UTF-8"?>
<manifest>

 <!-- Remotes -->
 <remote name="hub"
         fetch="https://github.com/dogpoopy/"
         clone-depth="1" />

 <remote name="lab"
         fetch="https://gitlab.com/dogpoopy/"
         clone-depth="1" />
 
 <!-- Trees -->
 <project path="device/xiaomi/vayu"
          name="android_device_xiaomi_vayu"
          remote="hub"
          revision="PixelOS-fourteen" />
 
 <project path="vendor/xiaomi/vayu"
          name="android_vendor_xiaomi_vayu"
          remote="hub"
          revision="main" />
 
 <project path="kernel/xiaomi/vayu"
          name="dogpoopy/android_kernel_xiaomi_vayu"
          remote="github"
          revision="main" />
 
 <project path="vendor/xiaomi/vayu-miuicamera"
          name="vendor_xiaomi_vayu-miuicamera"
          remote="lab"
          revision="main" />

 <project path="hardware/xiaomi"
          name="android_hardware_xiaomi"
          revision="pixelos-14"
          remote="hub" />

 <remove-project name="vendor_gms" />

 <remove-project name="vendor_aosp" />
 <project path="vendor/aosp"
          name="vendor_aosp"
          revision="fourteen"
          remote="hub" />

</manifest>
